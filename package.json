{
  "name": "maratongue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "dev:api": "export NO_D1_WARNING=true && wrangler dev ./renderer/api.ts",
    "build": "vite build",
    "preview": "vite build && wrangler dev",
    "deploy": "vite build && wrangler deploy",
    "db": "wrangler d1 execute csat --local --file=./design/schemas/schema.sql",
    "release": "release-it"
  },
  "dependencies": {
    "@ark-ui/vue": "^0.9.0",
    "@hono/zod-validator": "^0.1.11",
    "@lucia-auth/adapter-sqlite": "^2.0.1",
    "@lucia-auth/oauth": "^3.5.1",
    "@vue/server-renderer": "^3.3.13",
    "clientjs": "^0.2.1",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.5.1",
    "hono": "^3.11.9",
    "lucia": "^2.7.6",
    "maska": "^2.1.10",
    "qrcode.vue": "^3.4.1",
    "tailwind-variants": "^0.1.19",
    "typescript": "^5.3.3",
    "vee-validate": "^4.12.3",
    "vike": "^0.4.158",
    "vue": "^3.3.13",
    "vue-turnstile": "^1.0.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/clientjs": "^0.2.2",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/compiler-sfc": "^3.3.13",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.23",
    "drizzle-kit": "^0.19.13",
    "postcss": "^8.4.32",
    "release-it": "^16.3.0",
    "tailwindcss": "^3.4.0",
    "vite": "^4.5.1",
    "wrangler": "^3.22.1"
  },
  "imports": {
    "#design/*": "./design/*",
    "#framework/*": "./renderer/*",
    "~/*": "./src/*"
  },
  "type": "module",
  "release-it": {
    "git": {
      "commit": true,
      "tag": true,
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "Release ${version}"
    },
    "npm": {
      "publish": false
    }
  }
}
